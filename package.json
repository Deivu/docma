{
  "name": "docma",
  "version": "2.0.0",
  "description": "A powerful dev-tool to easily generate beautiful HTML documentation from Javascript (JSDoc), Markdown and HTML files.",
  "repository": "onury/docma",
  "license": "MIT",
  "author": {
    "name": "Onur Yıldırım",
    "email": "onur@cutepilot.com"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "lib",
    "bin",
    "LICENSE"
  ],
  "bin": {
    "docma": "bin/docma.js"
  },
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "docs": "node bin/docma",
    "zebra-then-docs": "cd ./templates/zebra && npm run build && cd ../.. && npm run docs",
    "test": "jest --verbose --no-cache"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/lib",
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/test/(*.)?(spec|test).js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "/backup/",
      "/bin/",
      "/design/",
      "/doc/",
      "/templates/"
    ]
  },
  "keywords": [
    "doc",
    "docs",
    "jsdoc",
    "document",
    "documentation",
    "api",
    "source",
    "code",
    "javascript",
    "markdown",
    "html",
    "node",
    "dustjs",
    "generate",
    "build",
    "comments",
    "template",
    "dogma"
  ],
  "dependencies": {
    "bluebird": "^3.5.0",
    "chalk": "^2.4.1",
    "docma-template-zebra": "^2.0.0",
    "dustjs-linkedin": "^2.7.5",
    "easy-table": "^1.1.1",
    "express": "^4.16.3",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.1",
    "gzip-size": "^4.1.0",
    "inquirer": "^5.2.0",
    "jsdoc-x": "^3.0.0",
    "jsdom": "^11.9.0",
    "less": "^3.0.2",
    "less-plugin-clean-css": "^1.5.1",
    "lodash": "^4.17.10",
    "marked": "^0.3.19",
    "npm-name": "^3.2.0",
    "semver": "^5.5.0",
    "serve-static": "^1.13.2",
    "strip-json-comments": "^2.0.1",
    "uglify-js": "^3.3.23",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-xo": "^0.22.0",
    "jest-cli": "^22.4.3"
  }
}
